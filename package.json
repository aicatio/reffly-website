{
  "name": "gatsby",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "prestart": "run-s cleantmp copyimgs",
    "start": "gatsby develop",
    "prebuild": "run-s cleantmp copyimgs",
    "build": "gatsby build",
    "postbuild": "rm -rf deploy && cp -R public deploy",
    "copyimgs": "cp -R node_modules/@aicat/reffly/build/static/images static/images",
    "cleantmp": "rm -rf static/images .cache",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@aicat/reffly": "^1.0.5",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.2",
    "gatsby": "^3.14.1",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-mini-css-class-name": "^4.2.1", 
    "gatsby-plugin-postcss": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  }
}
